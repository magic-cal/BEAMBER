{
  "name": "amber",
  "version": "2.0.0",
  "description": "",
  "private": true,
  "author": "",
  "scripts": {
    "serve": "concurrently \"npm run dev:server\" \"npm run dev:client\" --raw",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "dev:client": "vue-cli-service serve",
    "build:client": "vue-cli-service build",
    "dev:server": "nodemon --config ./nodemon.config.json",
    "build:server": "rimraf dist/server && tsc -p tsconfig.server.json",
    "debug": "nodemon --config ./nodemon.config.json",
    "lint": "vue-cli-service lint",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "copy-swagger-ui": "ncp ./src/swagger-ui ./dist/swagger-ui",
    "tsoa": "tsoa",
    "gen": "npm run swagger-gen && npm run routes-gen && npm run fe",
    "fe": "npx openapi-generator-cli generate -g typescript-fetch -i ./utils/swagger.json -o ./src/api"
  },
  "dependencies": {
    "@js-joda/core": "^3.2.0",
    "@openapitools/openapi-generator-cli": "^2.2.2",
    "@types/swagger-jsdoc": "^3.0.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^5.0.0-alpha.8",
    "glob-parent": "^5.1.2",
    "hosted-git-info": "^4.0.2",
    "moment": "^2.29.1",
    "npm": "^7.6.3",
    "pg": "^8.5.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.6.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-cli-plugin-vuetify": "^2.1.0",
    "vue-easy-dnd": "^1.10.2",
    "vue-ganttastic": "^0.9.32",
    "vue-i18n": "^8.23.0",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.5.1",
    "vuetify": "^2.4.3",
    "vuetify-loader": "^1.7.1",
    "vuex": "^3.1.3",
    "vuex-module-decorators": "^1.0.1"
